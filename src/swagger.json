              "type": "string"
            }
          },
          "user": {
            "type": "string"
          },
          "company": {
            "type": "string"
          },
          "price": {
            "type": "number"
          },
          "thumbnail": {
            "type": "string"
          },
          "thumbnail_url": {
            "type": "string"
          }
        }
      },
      "Unauthorized": {
        "properties": {
          "statusCode": {
            "type": "integer"
          },
          "error": {
            "type": "string"
          },
          "message": {
            "type": "string"
          },
          "attributes": {
            "type": "object",
            "properties": {
              "code": {
                "type": "integer"
              },
              "error": {
                "type": "string"
              }
            }
          },
          "docs": {
            "type": "string"
          }
        }
      },
      "Error": {
        "properties": {
          "statusCode": {
            "type": "integer"
          },
